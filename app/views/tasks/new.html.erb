<%= form_for :task, url: tasks_path do |f| %>

  <p>
    <%= f.label :name %>: <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :complete %>: <%= f.text_field :complete %>
  </p>

  <p>
    <%= f.label :due_date %>: <%= f.text_field :due_date %>
  </p>

  <p>
    <%= f.label :term %>: <%= f.text_field :term %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>

<% end %>